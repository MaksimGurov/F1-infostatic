<div class="container">
  <h1>F1 information</h1>
  <button class="btn btn-lg btn-danger">Welcome to Your F1 information, Max.</button>
  <div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="60"
         aria-valuemin="0" aria-valuemax="100" style="width: 40% ">
      <span class="sr-only">48% Complete</span>
    </div>
  </div>


  <table class="table table-striped">
    <thead>
    <tr>
      <th>Races </th>
      <th>Date </th>
      <th>Location </th>
      <th>Info</th>
    </tr>
    </thead>

    <tbody>
    <% @data.each do |element| %>
      <tr>
        <td><%= element[:raceName]%></td>
        <td><%= element[:raceDate]%></td>
        <td><%= element[:raceLocation]%></td>
        <td><%= element[:raceStatus] %></td>
        <td><%= link_to "Info", element[:raceURL] %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>